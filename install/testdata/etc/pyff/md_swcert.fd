- load:
   - /var/md_source
#- select: "!//md:EntityDescriptor[md:SPSSODescriptor]"
- select
- store:
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: your_metadatafeed_prefix_
- sign:
     key: /etc/pki/sign/private/metadata_key.pem
     cert: /etc/pki/sign/private/metadata_key.pem
- publish: /var/md_feed/metadata.xml
