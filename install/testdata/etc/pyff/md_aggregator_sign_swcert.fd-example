- load:
   - /var/md_source
#- select: "!//md:EntityDescriptor[md:SPSSODescriptor]"
- select
- store:
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: OurFederationSignedMetadata_
- sign:
    key: /etc/pki/sign/private/metadata_key.pem
    cert: /etc/pki/sign/certs/metadata_crt.pem
- publish: /var/md_feed/metadata.xml
