- load:
  - /var/md_source
- select
- xslt:
    stylesheet: unsign.xsl
- xslt:
    stylesheet: tidy.xsl
- store: 
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: your_metadatafeed_prefix_
- sign:
    # pkcs11URI: pkcs11://<token-library-path>/<tokenname>
    # Token password from ENV PYKCS11PIN
    key: pkcs11:///usr/lib64/libeToken.so/test
- publish: /var/md_feed/metadata.xml