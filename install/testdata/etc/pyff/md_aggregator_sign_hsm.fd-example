- load:
  - /var/md_source
#- select: "!//md:EntityDescriptor[md:SPSSODescriptor]"
- select
- xslt:
    stylesheet: unsign.xsl
- xslt:
    stylesheet: tidy.xsl
- store: 
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: your_metadatafeed_prefix_
- sign:
    # pkcs11URI: pkcs11://<token-library-path>/<tokenname>
    key: pkcs11:///usr/lib64/libeToken.so/test
    # Token PW from ENV PYKCS11PIN
    cert: /etc/pki/sign/certs/metadata_crt.pem
- publish: /var/md_feed/metadata.xml