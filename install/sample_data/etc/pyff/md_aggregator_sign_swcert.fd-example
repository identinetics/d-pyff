- load:
   - /var/md_source
#- select: "!//md:EntityDescriptor[md:SPSSODescriptor]"
- select
- store:
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: OurFederationSignedMetadata_
- sign:
    key: /etc/pki/pyff/metadata_signing-key.pem
    cert: /etc/pki/pyff/metadata_signing-crt.pem
- publish: /var/md_feed/metadata.xml

