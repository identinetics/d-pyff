- load:
   - /var/md_source
#- select: "!//md:EntityDescriptor[md:IDPSSODescriptor]"
- select
- store:
     directory: /tmp/
- finalize:
     cacheDuration: PT4H
     validUntil: P10D
     ID: OurFederationSignedMetadata_
- sign:
    key: /etc/pki/metadata_key.pem
    cert: /etc/pki/metadata_crt.pem
- publish: /var/md_feed/metadata.xml

